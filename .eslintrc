{
  "extends": ["airbnb", "plugin:jsx-a11y/recommended", "prettier", "prettier/react"],
  "plugins": ["jsx-a11y", "prettier"],
  "rules": {
    "semi": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "prettier/prettier": [
      "error",
      {
        "semi": false
      }
    ],
    "no-restricted-globals": "off",
    "no-console": "off"
  },
  "env": {
    "browser": true,
    "node": true,
    "jasmine": true,
    "jest": true
  }
}
